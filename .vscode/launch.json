  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch hqd",
      "program": "${workspaceFolder}/index.mjs",
      "args": [
        "-r",
        "${workspaceFolder}/test/hqd_test"
      ],
      "runtimeArgs": [
        "--experimental-json-modules"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch hqd streaming mode",
      "program": "${workspaceFolder}/index.mjs",
      "args": [
        "-r",
        "${workspaceFolder}/test/hqd_test",
        "-s"
      ],
      "runtimeArgs": [
        "--experimental-json-modules"
      ]
    }
  ]
}
